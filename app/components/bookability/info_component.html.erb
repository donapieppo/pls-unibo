<div id="binfos" class="my-4 font-bold bg-blue-100 rounded-xl p-2">
  <% if @what.external_booking? %>  
    per le iscrizioni visitare il sito
    <%= link_to @what.booking_url, @what.booking_url %>  
  <% else %>
    <%= @what.bookable_by_description.upcase %><br/>
    dal <%= l @what.booking_start %>
    al <%= l @what.booking_end %>.
<% end %>
</div>

